185.
# i/p: AABBBCCCCDAAAAA
# o/p: A2B3C4DA5
from itertools import groupby

def encode_logic(string):
    return '\t'.join(f"{char} {len(list(group))}" for char,group in groupby(string))
   
a="AABBBCCCCDAAAAA" 
output=encode_logic(a)
print(output)

