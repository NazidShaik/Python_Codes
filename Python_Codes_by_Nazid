#List Methods
list=[2,1,3]
list.append(4)   #adds one element at the end [2,1,3,4]
list.sort()  [1,2,3]
list.sort(reverse=True) #sorts in descending order [3,2,1]
list.reverse() #reverses list [3,1,2]
list.index(idx,el)  #insert element in index
list.remove(1)  #[2,3]
list.pop(idx)

60.
list=[2,1,3]
list.append(31)
print(list)
o/p:[2, 1, 3, 31]

list=[2,1,3]
list.sort()
print(list)
o/p:[1, 2, 3]

list=[2,1,3,46,123,24]
list.sort(reverse=True)
print(list)
o/p:[123, 46, 24, 3, 2, 1]

list=[2,1,3,46,123,24]
list.reverse()
print(list)
o/p:[24, 123, 46, 3, 1, 2]

