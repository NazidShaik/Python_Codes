193.#Escape Characters

txt = "It\'s alright" 
print(txt)
o/p:It's alright

txt = "It\\'s alright" 
print(txt)
o/p:It\'s alright

txt = "Hello\nWorld"
print(txt)
o/p:
Hello
World

txt = "Hello\rworld"
print(txt)
o/p:
Hello
world

txt = "Nazid\tPython\tProgrammer"
print(txt) 
o/p:Nazid	Python	Programmer

